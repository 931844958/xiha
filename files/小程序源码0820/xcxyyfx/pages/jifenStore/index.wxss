.banner-wrapper{
    position: relative;
    height: 137rpx;
    padding:30rpx;
    box-sizing: content-box;
    color: #ffffff;
}
.banner-bg{
    position: absolute;
    z-index: -11;
    top:0;
    left:0;
    width: 100%;
    height: 100%;
}
.store-image{
    width: 30rpx;
    height: 26rpx;
}
.count-num{
    font-size: 32rpx;
    margin:0 10rpx;
}
.count-name{
    font-size: 26rpx;
}
.detail-href{
    font-size: 22rpx;
    line-height: 3;
}
.lishi-btn{
    position: absolute;
    top:50%;
    transform: translateY(-50%);
    right:25rpx;
    height: 52rpx;
    width: 153rpx;
    border-radius: 26rpx;
    text-align: center;
    font-size: 22rpx;
    border:1rpx solid #ffffff;
    line-height: 2.4;
}

.title{
    height: 83rpx;
    line-height: 2;
    padding-left:33rpx;
    border-bottom:1rpx solid #eeeeee;
}
.title text{
    border-left:2px solid #F62D55;
    padding-left: 20rpx;
    color:#333333;
    font-size: 28rpx;
}
/*商品列表*/
.good_list{
    display: flex;
    flex-wrap: wrap;
}
._goods{
    padding:10px 14px;
    width: 41%;
    box-sizing: content-box;
    border-right: 1px solid #eeeeee;
    border-bottom: 1px solid #eeeeee;
}
._goods:nth-child(2n){
    border-right: none;
}
.good-item{
    position: relative;
    width:100%;
    height:194rpx;
    background-color: #f3f3f3;
    border-radius: 10px;
    padding-top:10px;
    text-align: center;
}
.iphoneImg{
    height:90%;
    max-width: 100%;
}
._store{
    position: absolute;
    right:0;
    top:0;
    background-color: #31c9ab;
    opacity: 0.6;
    height:42rpx;
    width: 101rpx;
    line-height: 1.8;
    text-align: center;
    border-bottom-left-radius: 20rpx;
    color:#ffffff;
    font-size:10px;
}
.over_store{
    background-color: #999999;
}
.overImg{
    position: absolute;
    right:10rpx;
    bottom:10rpx;
    width: 90rpx;
    height: 90rpx;
}
.good_name{
    color:#333333;
    font-size: 16px;
    text-align: left;
    line-height: 2;
    border-bottom: 1px solid #eeeeee;
}
.btn-wrapper{
    width:187rpx;
    height:56rpx;
    line-height: 2;
    text-align: center;
    color:#e8454e;
    font-size: 26rpx;
    border: #e8454e 1px solid;
    border-radius: 28rpx;
    margin:20rpx auto;
}
._history{
    display: flex;
    justify-content: space-between;
    color: #999999;
    padding-top:.225rem;
    font-size: 12px;
}
._history image{
    width: 26rpx;
    height: 22rpx;
    margin-right: 2px;
}
._history    .jifen{
    color:#e8454e;
    margin-left: 2px;
}
 ._history   .yiduihuan{
    color: #333333;
}