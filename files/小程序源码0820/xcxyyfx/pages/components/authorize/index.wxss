.container{
    height: 100vh;
}
.get-info{
    width: 390rpx;
    line-height: 2.2;
    text-align: center;
    box-sizing: border-box;
    background-color: #e81f0b;
    color: #ffffff;
    font-size: 36rpx;
}
.head-wrapper{
    height: 400rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.icon{
    width:68rpx;
    height:56rpx;
    vertical-align: middle;
}
.logo{
    width:160rpx;
    height:160rpx;
    border-radius: 50%;
}
.brand{
    line-height: 2;
    color: #333333;
    text-align: center;
    font-size: 20px;
    font-family: PingFang-SC-Medium;
}