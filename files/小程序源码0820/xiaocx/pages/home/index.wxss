.container{
  background: #FFECEB;
}
.header-bg{
  background: #FF3826;height: 188rpx;
}
.block{
  margin-bottom: 20rpx;width: 710rpx;margin: 20rpx auto;background: #ffffff;
}
.block:first-of-type{
  margin-top:0;
}
.header{
  margin-top:-188rpx;
  height: 188rpx;
}
.user-container{
  display: flex;
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  position: relative;
}
.user-container .navigator{
  padding: 10px;
  position: absolute;
  top:30rpx;
  right: 30rpx;
}
.user-container .products{
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
}
.user-container .icon-wrapper{
  width: 188rpx;
  height: 188rpx;
  border-radius: 50%;
  border: 4rpx solid #ffffff;
}
.user-container .icon-wrapper .image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.user-container .content{
  flex: 1;
  position: relative;
  padding: 30rpx 20rpx;
}
.user-container .content .name{
  color:#d93e36;
  font-family: PingFang-SC-Medium;
  font-size: 30rpx;
  line-height: 2;
}
.user-container .content .money{
  color:#666666;
  font-family: PingFang-SC-Regular;
  font-size: 24rpx;
  line-height: 2;
}
.user-container .content .userGrade{
  color:#666666;
  font-family: PingFang-SC-Regular;
  font-size: 26rpx;
  line-height: 1.5;
}


.extract-wrapper{
  padding-top:20rpx;
}
.swiper-extract{
  height: 188rpx !important;
}
.extract-item{
  padding:0 2rpx;
  width: 140rpx !important;
  font-family: PingFang-SC-Regular;
  text-align: center;
}
.extract-item .image{
  width: 80rpx;height: 80rpx;border-radius: 50%;
}
.extract-item text{
  line-height: 1.5;width: 100%;display: block;overflow: hidden;white-space: nowrap;
}
.extract-item .nickname{
  font-size: 24rpx;
  color: #666666;
}
.extract-item .time,.extract-item .desc{
  font-size: 20rpx;color: #999999;
}
.banner-wrapper{
  padding: 30rpx 0;
}
.banner-item .image{
  width:670rpx;
  height: 280rpx;
  border-radius: 8rpx;
  display: block;
  margin:0 auto;
}
.banner-item .button{
  background-color: #ffffff;
}
.banner-item .button::after{
  border: none;
}
.block .title{
  padding:20rpx 0;
  font-size: 30rpx;
  color:#333333;
}
.block .title text{
  padding-left: 12rpx;
  position: relative;
}
.block .title text::before{
  content: '';
  position: absolute;
  top:50%;
  transform: translateY(-50%);
  left: 0;
  width: 6rpx;
  height: 20rpx;
  background: #d93e36;
}

