/*案例*/
.caseItem{
  padding:20rpx 30rpx 0 20rpx;
}
.caseItem .name-icon{
  display: flex;
  align-items: center;
}
.caseItem .name-icon .image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 12rpx;
}
.caseItem .name-icon .text{
  font-size: 30rpx;
  color: #333333;
}
.caseItem .title{
  padding:20rpx 0;
  font-size: 24rpx;
}
.caseItem .image-wrapper{
  display: flex;
  justify-content: space-between;
  padding-bottom: 20rpx;
  border-bottom:solid 1rpx #eeeeee; 
}
.caseItem .image-wrapper image{
  width: 200rpx;
  height: 200rpx;
}