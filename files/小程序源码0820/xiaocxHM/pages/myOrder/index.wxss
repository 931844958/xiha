page{
    height: 100%;
}
.item{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
  border-bottom: 1rpx solid #eeeeee;
}
.desc{
  font-size: 30rpx;
  line-height: 2;
  color: #333333;
  font-family: PingFang-SC-Medium;
}
.tiem{
  font-size: 24rpx;
  color: #b7b7b7;
  font-family: PingFang-SC-Medium;
}
.item:last-of-type{
  border-bottom: none;
}
.jiantou{
  transform:rotate(-45deg);
  width: 18rpx;
  height: 18rpx;
  border-right: 1px solid #888888;
  border-bottom: 1px solid #888888;
}